#include<stdlib.h>
#include<stdio.h>

void insertion(int arr[], int len)
{
	int i, j;
	int temp;
	//int count;
	
	for (i=0;i<len-1;i++)
	{
		//count = 0;
		j = i+1;
		while ((j>0) && (arr[j]<arr[j-1]))
		{
			temp = arr[j-1];
			arr[j-1] = arr[j];
			arr[j] = temp;
			j--;
			//count++;
		}
		//printf("count = %d\n", count);
	}
}
		
